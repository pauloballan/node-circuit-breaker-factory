{
  "name": "@springworks/circuit-breaker-factory",
  "description": "Factory module for creating circuit breakers, to be used with any type of callback.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "NODE_ENV=test istanbul cover _mocha -- test",
    "lint": "eslint --cache .",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commit": "commit-wizard",
    "postpublish": "greenkeeper-postpublish"
  },
  "dependencies": {
    "@springworks/input-validator": "4.1.0",
    "circuit-breaker-js": "0.0.1"
  },
  "devDependencies": {
    "@springworks/test-harness": "1.5.1",
    "bunyan": "1.8.5",
    "coveralls": "2.11.15",
    "eslint": "3.11.0",
    "eslint-config-springworks": "7.0.7",
    "eslint-plugin-import": "2.1.0",
    "eslint-plugin-mocha": "4.7.0",
    "eslint-plugin-springworks": "2.0.1",
    "greenkeeper-postpublish": "1.0.1",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "3.2.0",
    "pre-git": "3.11.1",
    "semantic-release": "6.3.2"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=2.7"
  },
  "config": {
    "pre-git": {
      "commit-msg": "conventional"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Springworks/node-circuit-breaker-factory.git"
  },
  "keywords": [
    "stability",
    "circuit-breaker"
